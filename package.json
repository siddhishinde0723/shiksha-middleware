    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.9.0",
    "cache-manager": "^5.5.2",
    "cache-manager-memory-store": "^1.1.0",
    "class-transformer": "^0.5.1",
